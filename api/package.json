{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=development jest --runInBand --detectOpenHandles --forceExit",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -f ormconfig -c migrations",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -c migrations -d src/migrations -n",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig -c migrations",
    "test-migration:run": "ts-node node_modules/typeorm/cli.js migration:run -f ormconfig -c migrations-test",
    "test-migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig -c migrations-test"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "moment": "^2.29.2",
    "nodemailer": "^6.7.2",
    "nodemailer-sendgrid": "^1.0.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "postgres-interval": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "rxjs-compat": "^6.6.7",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.44",
    "typeorm-fixtures-cli": "^1.11.1",
    "typeorm-naming-strategies": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.10",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^27.5.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 30000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json",
        "isolatedModules": true
      }
    },
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ]
  }
}
